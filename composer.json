  "require": {
    "php": "^8.1",
    "briofy/repository-laravel": "dev-main",
    "briofy/rest-laravel": "dev-main"
  },
  "require-dev": {
    "laravel/pint": "^1.9"
  },
  "autoload": {
    "psr-4": {
      "Briofy\\Meta\\": "src/"
    }
