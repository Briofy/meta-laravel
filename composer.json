{
  "name": "briofy/meta-laravel",
  "description": "The official repository of Meta (key:value) module from Briofy for Laravel",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Muhammad",
      "email": "hi@trader4.net"
    }
